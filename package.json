{
  "name": "script",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=4096 --loader=ts-node/esm' ts-node ./src/main.ts",
    "prisma:gen:pg": "prisma generate --schema prisma-pg/schema.prisma",
    "prisma:dbpush:pg": "prisma db push --config ./prisma-pg/prisma.config.ts",
    "prisma:gen:mysql": "prisma generate --schema prisma-mysql/schema.prisma",
    "prisma:gen:all": "npm run prisma:gen:pg && npm run prisma:gen:mysql",
    "website:output": "tsx ./src/lib/website-output.ts --results-directory"
  },
  "dependencies": {
    "@prisma/adapter-pg": "6.18.0-integration-next.11",
    "@prisma/client": "6.18.0-integration-next.11",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "fast-csv": "^5.0.1",
    "mysql2": "^3.10.2",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/faker": "^6.6.9",
    "drizzle-kit": "^0.22.7",
    "pg-types": "^4.1.0",
    "prisma": "6.18.0-integration-next.11",
    "ts-node": "10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.5.2"
  }
}
